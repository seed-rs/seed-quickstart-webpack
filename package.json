  "scripts": {
    "start": "yarn generate:css_classes && webpack-dev-server --config configs/webpack.config.js -d",
    "build": "yarn generate:css_classes && webpack --config configs/webpack.config --mode=development",
    "build:release": "yarn generate:css_classes && webpack --config configs/webpack.config --mode=production && yarn optimize_wasm",
    "test": "yarn test:node",
    "test:node": "wasm-pack test ./crate --node",
    "test:chrome": "wasm-pack test ./crate --chrome",
    "test:firefox": "wasm-pack test ./crate --firefox",
    "test:safari": "wasm-pack test ./crate --safari",
    "optimize_wasm": "node scripts/optimize_wasm.js",
    "generate:css_classes": "webpack --config configs/webpack.css_classes.config --mode=development"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "1.0.0",
    "binaryen": "^87.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "find": "^0.3.0",
    "html-webpack-plugin": "^3.2.0",
    "no-emit-webpack-plugin": "^2.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-typed-css-classes": "^0.1.3",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.0.5",
    "text-encoding": "^0.7.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpackbar": "^3.2.0"
